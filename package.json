{
    "name": "mas-tools-api",
    "displayName": "MAS Tools API",
    "description": "Visual Studio Code extension for MAS Tools API by Reliability Odyssey",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:mas-tools-api.setup",
        "onCommand:mas-tools-api.generateIntegrityCheckerReport",
        "onCommand:mas-tools-api.getToolsLogs",
        "onCommand:mas-tools-api.submitUploadLogRequest",
        "onCommand:mas-tools-api.runIntegrityCheckerRepair",
        "onCommand:mas-tools-api.stopManagePods",
        "onCommand:mas-tools-api.startManagePods",
        "onCommand:mas-tools-api.installExternalCertificate"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mas-tools-api.setup",
                "title": "MAS Tools API: Setup"
            },
            {
                "command": "mas-tools-api.generateIntegrityCheckerReport",
                "title": "MAS Tools API: Generate Integrity Checker Report"
            },
            {
                "command": "mas-tools-api.getToolsLogs",
                "title": "MAS Tools API: Get All Tools Logs"
            },
            {
                "command": "mas-tools-api.submitUploadLogRequest",
                "title": "MAS Tools API: Upload Logs to S3"
            },
            {
                "command": "mas-tools-api.runIntegrityCheckerRepair",
                "title": "MAS Tools API: Run Integrity Checker Repair"
            },
            {
                "command": "mas-tools-api.stopManagePods",
                "title": "MAS Tools API: Stop Manage Pods"
            },
            {
                "command": "mas-tools-api.startManagePods",
                "title": "MAS Tools API: Start Manage Pods"
            },
            {
                "command": "mas-tools-api.installExternalCertificate",
                "title": "MAS Tools API: Install External Certificate"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch --mode development --devtool source-map",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3",
        "ts-loader": "^9.5.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "@vscode/test-cli": "^0.0.4",
        "@vscode/test-electron": "^2.3.8"
    },
    "dependencies": {
        "axios": "^1.6.2"
    }
}